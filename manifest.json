{
  "manifest_version": 3,
  "name": "BadAds",
  "version": "1.3",
  "description": "Smart popup and new-tab ad blocker.",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "icons/logoOn.png",
      "48": "icons/logoOn.png",
      "128": "icons/logoOn.png"
    }
  }
}